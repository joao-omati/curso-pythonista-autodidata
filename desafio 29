from abc import ABC,abstractclassmethod

class Monitor(ABC):
    @abstractclassmethod
    def aumentar_claridade(self,numero):
        pass
    @abstractclassmethod
    def diminuir_claridade(self,numero):
        pass
        
class MonitorFullHD(Monitor):
     def aumentar_claridade(self,numero):
        print(f"aumentar a claridade em {numero}")
     
     def diminuir_claridade(self,numero):
       print(f"diminuir a claridade em {numero}")
